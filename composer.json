{
    "name": "flevnex/elicense",
    "version": "1.0.1",
    "description": "A Laravel package for managing electronic licenses",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Flevnex",
            "email": "elicense@flevnex.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Flevnex\\Elicense\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flevnex\\Elicense\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flevnex\\Elicense\\ElicenseServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
